{
  "name": "discourse-example",
  "version": "1.0.0",
  "description": "Example bot for the discourse framework",
  "main": "basicBot.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --experimental-specifier-resolution=node basicBot.js",
    "test": "echo \"Error: no test specified\" && exit 1",
